# Figure-MCP Server 환경 설정

# 로그 레벨 (debug, info, warn, error)
LOG_LEVEL=info

# 실행 환경 (development, production)
NODE_ENV=development

# Figure-MCP Platform API 설정
FIGURE_MCP_PLATFORM_URL=http://localhost:8000
PLATFORM_API_KEY=your-platform-api-key-here
MCP_SERVER_ID=figure-mcp-server

# 개발 환경에서는 localhost 사용
# 프로덕션 환경에서는 실제 플랫폼 URL로 변경
# 예: FIGURE_MCP_PLATFORM_URL=https://api.figure-mcp.com 